<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aman.pccontrol">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Server" />
        <activity android:name=".TestActivity" />
        <activity
            android:name=".ToPCWithNav"
            android:label="@string/title_activity_to_pcwith_nav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MouseActivity" />
        <activity android:name=".PowerOFF" />
        <activity android:name=".KeyboardActivity" />
        <activity android:name=".Presenter" />
        <activity android:name=".FileTransfer" />
        <activity android:name=".FileDownload"></activity>
    </application>

</manifest>